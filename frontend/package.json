    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "sass": "^1.62.1",
    "svelte": "^3.57.0",
    "svelte-check": "^2.10.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
